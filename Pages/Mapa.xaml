<?xml version="1.0" encoding="utf-8"?>

<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             xmlns:localClass="clr-namespace:Projekt.Class"
             x:Class="Projekt.Pages.Mapa">
    <ContentPage.Content>
        <StackLayout x:Name="widokMapy">
            <localClass:BindableMap MapType="Street" MapPosition="{Binding MyPositions}" MapPins="{Binding PinCollections}"/>
        </StackLayout>
    </ContentPage.Content>
</ContentPage>