    </EmbeddedResource>
    <EmbeddedResource Update="Pages\Walka.xaml">
      <Generator>MSBuild:UpdateDesignTimeXaml</Generator>
    </EmbeddedResource>
  </ItemGroup>

  <ItemGroup>
